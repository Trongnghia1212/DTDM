@model IEnumerable<QLNhanSu.Models.ChucVu>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<h2>Chức Vụ</h2>

<p>
    @Html.ActionLink("Thêm mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenCV)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenCV)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.MaCV }) |
            @Html.ActionLink("Chi Tiết", "Details", new { id=item.MaCV }) |
            @Html.ActionLink("Xóa", "Delete", new { id=item.MaCV })
        </td>
    </tr>
}

</table>
